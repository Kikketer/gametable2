{
  build: {
    beforeDevCommand: 'yarn dev',
    beforeBuildCommand: 'yarn build',
    devPath: 'http://localhost:5173',
    distDir: '../dist',
    withGlobalTauri: false,
  },
  package: {
    productName: 'GameboardScreenTwo',
    version: '0.0.0',
  },
  tauri: {
    allowlist: {
      all: false,
      dialog: {
        all: false,
        open: true
      },
      fs: {
        all: false,
        readFile: true,
        scope: ['$DOCUMENT/GameBoard/**/*']
      },
      shell: {
        all: false,
        open: true,
      },
    },
    bundle: {
      active: true,
      icon: [
        'icons/32x32.png',
        'icons/128x128.png',
        'icons/128x128@2x.png',
        'icons/icon.icns',
        'icons/icon.ico',
      ],
      identifier: 'com.cjweed.gametabletwo',
      targets: 'all',
      macOS: {
        minimumSystemVersion: '10.14'
      }
    },
    security: {
      csp: null,
    },
    updater: {
      active: false,
    },
    windows: [
      {
        fullscreen: false,
        resizable: true,
        title: 'Controller',
        width: 800,
        height: 600,
        url: 'controller'
      },
    ],
  },
}